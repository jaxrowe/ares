n                    vels = [[vels[x], vels[x+1], vels[x+2]] for x in range(0, numpos, 3)]\n                    ptcacheprim.GetVelocitiesAttr().Set(vels, frame)\n                \n                if "w" in attribnames:\n                    vels = geo.pointFloatAttribValues("w")\n                    vels = [[vels[x], vels[x+1], vels[x+2]] for x in range(0, numpos, 3)]\n                    angvel = ptcacheprim.GetPrim().GetAttribute("angularVelocities")\n                    if not angvel:\n                        ptcacheprim.GetPrim().CreateAttribute("angularVelocities", Sdf.ValueTypeNames.Float3Array)\n                    angvel.Set(vels, frame)\n                \n                if "accel" in attribnames:\n                    accels = geo.pointFloatAttribValues("accel")\n                    accels = [[accels[x], accels[x+1], accels[x+2]] for x in range(0, numpos, 3)]\n                    ptcacheprim.GetAccelerationsAttr().Set(accels, frame)\n                \n                pv_api = UsdGeom.PrimvarsAPI(ptcacheprim.GetPrim())\n                \n                names = geo.pointStringAttribValues(pieceattrib if pieceattrib in attribnames else "name")\n                pv = pv_api.CreatePrimvar(pieceattrib, Sdf.ValueTypeNames.StringArray, "vertex")\n                pv.Set(names)\n                \n                orients = geo.pointFloatAttribValues("orient")\n                orients = [[orients[x], orients[x+1], orients[x+2], orients[x+3]] for x in range(0, len(orients), 4)]\n                pv = pv_api.CreatePrimvar("orient", Sdf.ValueTypeNames.Float4Array, "vertex")\n                pv.Set(orients, frame)\n                \n                scales = geo.pointFloatAttribValues("scale")\n                scales = [[scales[x], scales[x+1], scales[x+2]] for x in range(0, numpos, 3)]\n                pv = pv_api.CreatePrimvar("scale", Sdf.ValueTypeNames.Float3Array, "vertex")\n                pv.Set(scales, frame)\n                \n                pivots = geo.pointFloatAttribValues("pivot")\n                pivots = [[pivots[x], pivots[x+1], pivots[x+2]] for x in range(0, numpos, 3)]\n                pv = pv_api.CreatePrimvar("pivot", Sdf.ValueTypeNames.Float3Array, "vertex")\n                pv.Set(pivots, frame)\n            \n' ) maintainstate ( off )
